{
    "name": "fioriyoeman",
    "version": "1.0.0",
    "description": "Yoeaman template",
    "main": "index.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "start": "fiori run --open index.html",
        "start-dist": "fiori run --open index.html --config ui5-dist.yaml",
        "start-mock": "fiori run --config ui5-mock.yaml --open test/mockserver.html",
        "build": "ui5 build -a --clean-dest --include-task=generateManifestBundle generateCachebusterInfo",
        "deploy": "npm run build && fiori deploy --config ui5-deploy.yaml && rimraf archive.zip",
        "undeploy": "fiori undeploy --config ui5-deploy.yaml"
    },
    "author": "gowri",
    "license": "ISC",
    "dependencies": {
        "@ui5/cli": "^2.12.1",
        "express": "^4.17.1"
    },
    "devDependencies": {
        "@sap/ux-specification": "^1.90.9",
        "@sap/ux-ui5-tooling": "^1.3.3",
        "@ui5/cli": "^2.12.1",
        "rimraf": "3.0.2"
    },
    "ui5": {
        "dependencies": [
            "@sap/ux-ui5-tooling"
        ]
    }
}
